generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
  phone Int @unique
  nationalId Int @unique 
  password String 
}

model Post {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  postName String @unique
}
